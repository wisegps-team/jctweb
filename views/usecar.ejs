                    <div class="t_a_c m_b10 hide" id="statastic_query" style="">
                        <div class="d_i_b m_r10">
                            使用人：
                            <input type="text" class="input-medium search-query" style="width:80px" id="usename">
                        </div>
                        <div class="d_i_b m_r10">
                            车牌号：
                            <input type="text" class="input-medium search-query" style="width:80px" id="cph">
                            <span></span>
                            <button type="button" class="btn btn-default m_t0" id="add_j">加警</button>
                        </div>
                        <div class="d_i_b m_r10">
                            部门：
                            <select class="_select" id="select_depart">
                                <!-- <option value="1">所有</option>
                                <option value="2">政治处</option>
                                <option value="32">局领导</option> -->
                            </select>
                        </div>
                        <div class="d_i_b m_r10">
                            开始时间：
                            <div class="input-append date start_time  input-medium _select" style="width:120px" data-date-format="yyyy-mm-dd" data-link-field="dtp_input1">
                                <input id="start_time" class="input-medium w_p80" type="text" value="" style="width:80px">
                                <span class="add-on">
                                    <i class="icon-th"></i>
                                </span>
                            </div>
                        </div>
                        <div class="d_i_b m_r10">
                            结束时间：
                            <div class="input-append date end_time  input-medium _select" style="width:120px" data-date-format="yyyy-mm-dd" data-link-field="dtp_input1">
                                <input id="end_time" class="input-medium w_p80" type="text" value=""  style="width:80px">
                                <span class="add-on">
                                    <i class="icon-th"></i>
                                </span>
                            </div>
                        </div>
                        <div class="d_i_b m_r10">
                            状态：
                            <select class="_select" id="use_status">
                                <option value="">所有</option>
                                <option value="0">撤销</option>
                                <option value="1">审批中</option>
                                <option value="4">驳回</option>
                                <option value="5">通过</option>
                                <option value="6">还车</option>
                            </select>
                        </div>
                        <div class="d_i_b">
                            <button type="button" class="btn btn-default m_t0" id="query_button">查询</button>
                        </div>
                    </div>
                   
                    <div id="table_scroll" style="overflow-y:auto;margin-left:10px">
                        <table class="table table-hover">
                            <thead>
                                <tr>
                                    <th>编号</th>
                                    <th>事由</th>
                                    <th>使用人</th>
                                    <th>同行</th>
                                    <th>地址</th>
                                    <th>详细地址</th>
                                    <th>过夜</th>
                                    <th>车辆</th>
                                    <th>驾驶员</th>
                                    <th>状态</th>
                                    <th>操作</th>
                                </tr>
                            </thead>
                            <tbody id="table_info">
                                <tr>
                                    <td colspan="11" style="text-align:center">没有数据！</td>
                                </tr>
                            </tbody>
                        </table>
